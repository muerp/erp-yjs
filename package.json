{
  "name": "@editablejs/yjs-server",
  "version": "0.0.0",
  "description": "Yjs Server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rimraf ./dist && cpx ./package.json ./dist && tsc",
    "dev": "nodemon --watch node_modules --watch src ./src/index.ts",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/ws": "^8.5.10",
    "cpx": "^1.5.0",
    "nodemon": "2.0.20",
    "rimraf": "^5.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "license": "MIT",
  "dependencies": {
    "@editablejs/models": "^1.0.0",
    "@editablejs/yjs-protocols": "^1.0.0",
    "@editablejs/yjs-transform": "^1.0.0",
    "http": "^0.0.1-security",
    "lib0": "0.2.52",
    "lodash.debounce": "^4.0.8",
    "mongodb": "^5.0.1",
    "ws": "^8.17.1",
    "y-leveldb": "^0.1.2",
    "yjs": "13.5.46"
  }
}
